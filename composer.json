{
  "name": "telnowedge/freepbx-base",
  "description": "Helper to create module with the Symfony components like form validation, dependency injection...",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Boris Morel",
      "email": "boris.morel@telnowedge.com"
    },
    {
      "name": "Bernard Merindol",
      "email": "bernard.merindol@telnowedge.com"
    }
