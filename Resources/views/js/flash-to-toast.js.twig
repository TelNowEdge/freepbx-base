<script type="text/javascript">
 $(document).ready(() => {
   let time = 0;
   {% for label, messages in flashes %}

   {% if label is not same as('provision', 'noToast') %}
   {% for message in messages %}
   setTimeout(() => {
     fpbxToast('{{ message|e('js') }}', '{{ label }}', '{{ label }}');
   }, time);

   time += 1000;
   {% endfor %}
   {% endif %}
   {% endfor %}
 });
</script>
